{
  "expo": {
    "name": "OnChain",
    "slug": "expo-starter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "scheme": "onchain",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/landing.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "dev.privy.example",
      "associatedDomains": ["webcredentials:<your-associated-domain>"],
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.privy.example"
    },
    "notification": {
      "icon": "./assets/images/app_icon.png",
      "color": "#FE5F00"
    },
    "extra": {
      "privyAppId": "cmen0rumq0003lb0btfagk0s5",
      "privyClientId": "client-WY6PqXfLbW6xuDQbun2FSn9MGcmUZjtqCNcVA6VFzWg8B",
      "passkeyAssociatedDomain": "https://<your-associated-domain>"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "17.5"
          },
          "android": {
            "compileSdkVersion": 35
          }
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#FE5F00",
          "sounds": []
        }
      ],
       [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/landing.png",
          "resizeMode": "cover"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
